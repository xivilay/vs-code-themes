{
    "name": "Tomorrow Night VS",
    "tokenColors": [
        {
            "scope": ["meta.template.expression.js"],
            "settings": { "foreground": "#C5C8C6" }
        },
        {
            "scope": ["comment", "meta.tag.any"],
            "settings": { "foreground": "#969896", "fontStyle": "italic" }
        },

        {
            "scope": "string",
            "settings": { "foreground": "#B5BD68" }
        },
        {
            "scope": ["entity.name.tag", "meta.definition.variable"],
            "settings": { "foreground": "#CC6666" }
        },
        {
            "scope": ["constant", "variable.parameter", "variable.language"],
            "settings": { "foreground": "#DE935F" }
        },
        {
            "scope": ["entity.other.attribute-name"],
            "settings": { "foreground": "#F0C674" }
        },
        {
            "scope": ["storage", "storage.type", "keyword", "keyword.operator.new"],
            "settings": { "foreground": "#B294BB" }
        },
        {
            "scope": "keyword.operator",
            "settings": { "foreground": "#8abeb7" }
        },
        {
            "scope": ["meta.definition.function"],
            "settings": { "foreground": "#81a2be" }
        },
        {
            "scope": [
                "keyword.control.as.ts",
                "keyword.operator.optional.ts",
                "keyword.operator.type.annotation.ts",
                "meta.return.type.ts",
                "meta.type.annotation.ts",
                "support.type.primitive.ts",
                "storage.type.built-in.primitive.cpp",
                "storage.type.primitive.cpp",
                "meta.qualified_type.cpp",
                "entity.name.type.parameter.cpp"
            ],
            "settings": { "foreground": "#666666" }
        }
    ],
    "colors": {
        "editor.foreground": "#C5C8C6",
        "editor.background": "#263035",
        "editor.lineHighlightBackground": "#2B2D31",
        "statusBar.background": "#333333",
        "statusBar.foreground": "#CCCCCC",
        "terminal.ansiBlack": "#263035",
        "terminal.ansiRed": "#cc6666",
        "terminal.ansiGreen": "#b5bd68",
        "terminal.ansiYellow": "#f0c674",
        "terminal.ansiBlue": "#81a2be",
        "terminal.ansiMagenta": "#b294bb",
        "terminal.ansiCyan": "#8abeb7",
        "terminal.ansiWhite": "#969896",
        "terminal.ansiBrightBlack": "#263035",
        "terminal.ansiBrightRed": "#cc6666",
        "terminal.ansiBrightGreen": "#b5bd68",
        "terminal.ansiBrightYellow": "#f0c674",
        "terminal.ansiBrightBlue": "#81a2be",
        "terminal.ansiBrightMagenta": "#b294bb",
        "terminal.ansiBrightCyan": "#8abeb7",
        "terminal.ansiBrightWhite": "#C5C8C6"
    }
}
